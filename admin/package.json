    "@types/gulp-util": "^3.0.30",
    "@types/jasmine": "^2.5.40",
    "@types/node": "^6.0.58",
    "@types/protractor": "^4.0.0",
    "@types/rimraf": "0.0.28",
    "@types/run-sequence": "^0.0.28",
    "@types/selenium-webdriver": "^2.53.39",
    "@types/systemjs": "^0.19.33",
    "@types/yargs": "^6.5.0",
    "@types/zone.js": "^0.0.27",
